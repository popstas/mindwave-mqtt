{
  "name": "mindwave-web",
  "version": "0.8.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "vite --port 9302 --host",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx,vue}\" --quiet",
    "typecheck": "tsc --noEmit",
    "build": "vue-tsc --noEmit && vite build",
    "build-dev": "vite build",
    "build-pkg": "pkg . --out-path dist",
    "build:client": "vite build --outDir dist/client --ssrManifest",
    "build:server": "vite build --outDir dist/server --ssr src/entry-server",
    "build:ssr": "npm run build:client && npm run build:server"
  },
  "bin": {
    "mindwave-web": "src/index.js"
  },
  "pkg": {
    "assets": [
      "assets/**/*",
      "public/**/*"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "base-64": "^1.0.0",
    "compression": "^1.7.4",
    "d3": "^7.3.0",
    "element-plus": "^1.3.0-beta.5",
    "express": "^4.17.2",
    "mqtt": "^4.2.6",
    "node-hide-console-window": "^2.1.0",
    "node-thinkgear-sockets": "0.0.1",
    "nosleep.js": "^0.12.0",
    "open": "^8.4.0",
    "systray2": "^2.1.4",
    "vite-plugin-windicss": "^1.6.2",
    "vue-router": "^4.0.12",
    "vuex": "^4.0.2",
    "vuex-persistedstate": "^4.1.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.0.1",
    "@vitejs/plugin-vue-jsx": "^1.3.3",
    "sass": "^1.47.0",
    "typescript": "^4.5.4",
    "vite": "^2.7.10",
    "vite-plugin-pages": "^0.19.9",
    "vue": "^3.2.26",
    "vue-tsc": "^0.30.2"
  }
}
